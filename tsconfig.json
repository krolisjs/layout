{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "declaration": false,
    "emitDeclarationOnly": false,
    "incremental": true,
    "tsBuildInfoFile": "./node_modules/.tsbuildinfo",
    "removeComments": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "lib": [
      "dom",
      "webworker",
      "esnext"
    ]
  },
  "exclude": [
    "node_modules",
    "test"
  ],
}
